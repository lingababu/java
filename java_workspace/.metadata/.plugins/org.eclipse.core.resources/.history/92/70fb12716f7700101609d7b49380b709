package com.string;


public class CountOccurenceOfEachCharInString {

	public static void main(String[] args) {
		
		String str = "llb";

	}
	
	public static void countOccurences(String str){
		
		char[] data = str.toCharArray();
		
		int count=0;
		
		for(int i=0; i < data.length; i++) {
			
			count = data[i];
			
		}
		
	}

}
